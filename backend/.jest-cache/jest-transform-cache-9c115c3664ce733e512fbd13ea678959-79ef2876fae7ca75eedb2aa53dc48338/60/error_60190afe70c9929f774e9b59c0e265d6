/**
 * Função para verificar se um erro é operacional
 */
/* istanbul ignore next */
cov_2q3rzgh2ma().s[19]++;
const isOperationalError = error => {
  /* istanbul ignore next */
  cov_2q3rzgh2ma().f[7]++;
  cov_2q3rzgh2ma().s[20]++;
  if (error instanceof AppError) {
    /* istanbul ignore next */
    cov_2q3rzgh2ma().b[7][0]++;
    cov_2q3rzgh2ma().s[21]++;
    return error.isOperational;
  } else
  /* istanbul ignore next */
  {
    cov_2q3rzgh2ma().b[7][1]++;
  }
  cov_2q3rzgh2ma().s[22]++;
  return false;
};
